#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Administrator/Desktop/moltbot-main/node_modules/.pnpm/@mariozechner+jiti@2.6.5/node_modules/@mariozechner/jiti/lib/node_modules:/mnt/c/Users/Administrator/Desktop/moltbot-main/node_modules/.pnpm/@mariozechner+jiti@2.6.5/node_modules/@mariozechner/jiti/node_modules:/mnt/c/Users/Administrator/Desktop/moltbot-main/node_modules/.pnpm/@mariozechner+jiti@2.6.5/node_modules/@mariozechner/node_modules:/mnt/c/Users/Administrator/Desktop/moltbot-main/node_modules/.pnpm/@mariozechner+jiti@2.6.5/node_modules:/mnt/c/Users/Administrator/Desktop/moltbot-main/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Administrator/Desktop/moltbot-main/node_modules/.pnpm/@mariozechner+jiti@2.6.5/node_modules/@mariozechner/jiti/lib/node_modules:/mnt/c/Users/Administrator/Desktop/moltbot-main/node_modules/.pnpm/@mariozechner+jiti@2.6.5/node_modules/@mariozechner/jiti/node_modules:/mnt/c/Users/Administrator/Desktop/moltbot-main/node_modules/.pnpm/@mariozechner+jiti@2.6.5/node_modules/@mariozechner/node_modules:/mnt/c/Users/Administrator/Desktop/moltbot-main/node_modules/.pnpm/@mariozechner+jiti@2.6.5/node_modules:/mnt/c/Users/Administrator/Desktop/moltbot-main/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@mariozechner+jiti@2.6.5/node_modules/@mariozechner/jiti/lib/jiti-cli.mjs" "$@"
else
  exec node  "$basedir/../../../../../../@mariozechner+jiti@2.6.5/node_modules/@mariozechner/jiti/lib/jiti-cli.mjs" "$@"
fi
